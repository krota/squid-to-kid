{
	"manifest_version": 2,
	"name": "Squid to Kid",
	"version": "1.1",
	"description": "Replaces the text 'squid' with 'kid' and vise versa.",
	"icons": {
		"16"  : "icon16.png",
		"128" : "icon128.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

